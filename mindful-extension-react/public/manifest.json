{
  "name": "Mindful",
  "version": "0.1",
  "description": "Mindful Chrome Extension Demo",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "manifest_version": 2,
  "icons": {
    "16":  "favicons/favicon-16x16.png",
    "32":  "favicons/favicon-32x32.png",
    "48": "favicons/icon@8x.png",
    "128": "favicons/icon@8x.png"
  },
  "permissions": [
    "webNavigation",
    "tabs",
    "http://127.0.0.1:5000/"
  ]
}